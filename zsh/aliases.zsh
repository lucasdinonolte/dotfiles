alias git=hub
alias g=hub

# aliases for Tmux
alias tmux='tmux -2'
alias ta='tmux attach -t'
alias tnew='tmux new -s'
alias tls='tmux ls'
alias tkill='tmux kill-session -t'

# convenience aliases for editing configs
alias vim="$EDITOR"
alias ev='$EDITOR ~/.vimrc'
alias et='$EDITOR ~/.tmux.conf'
alias ez='$EDITOR ~/.zshrc'
alias v='$EDITOR .'
